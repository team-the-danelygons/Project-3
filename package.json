{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore client/\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "seed2": "node scripts/seedBiz.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build && node scripts/seedBiz.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.6.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap-horizon": "^1.0.1",
    "cors": "^2.8.5",
    "env-variable": "0.0.6",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "nocache": "^2.1.0",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^2.0.0-beta.7",
    "validator": "^13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-the-danelygons/Project-3.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/team-the-danelygons/Project-3/issues"
  },
  "homepage": "https://github.com/team-the-danelygons/Project-3#readme"
}
